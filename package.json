{
  "name": "uber-bot",
  "version": "0.0.1",
  "description": "Uber Discord Bot",
  "main": "uberbot",
  "author": "Crow08",
  "license": "MIT",
  "engines": {
    "node": "14.4.0"
  },
  "scripts": {
    "announcer": "node ./src/index.js disable_bot disable_web",
    "bot": "node ./src/index.js disable_announcer disable_web",
    "build": "npm install",
    "debug": "node ./src/index.js debug",
    "docs": "node_modules/.bin/jsdoc -r -c jsdoc.json",
    "start": "node ./src/index.js",
    "tools_cmdTable": "node ./src/tools/createCmdTable.js",
    "web": "node ./src/index.js disable_announcer disable_bot"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crow08/discord-uberbot.git"
  },
  "dependencies": {
    "bufferutil": "^4.0.1",
    "discord.js": "^12.2.0",
    "erlpack": "^0.1.3",
    "ffmpeg-static": "^4.2.2",
    "libsodium": "^0.7.6",
    "mongodb": "^3.5.8",
    "node-opus": "^0.3.3",
    "node-spotify-api": "^1.1.1",
    "prism-media": "^1.2.2",
    "request": "^2.88.2",
    "utf-8-validate": "^5.0.2",
    "ytdl-core-discord": "github:Crow08/ytdl-core-discord",
    "ytpl": "^0.1.21",
    "zlib-sync": "^0.1.6",
    "zucc": "^0.1.2"
  },
  "devDependencies": {
    "better-docs": "^1.4.13",
    "eslint": "^6.8.0"
  }
}
