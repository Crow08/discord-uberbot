{
  "name": "uber-bot",
  "version": "0.0.1",
  "description": "Uber Discord Bot",
  "main": "uberbot",
  "author": "Crow08",
  "license": "MIT",
  "engines": {
    "node": "16.11.0",
    "npm": "8.14.0"
  },
  "scripts": {
    "announcer": "node ./src/index.js disable_bot disable_web",
    "bot": "node ./src/index.js disable_announcer disable_web",
    "build": "npm install",
    "debug": "node ./src/index.js debug",
    "docs": "node_modules/.bin/jsdoc -r -c jsdoc.json",
    "start": "node ./src/index.js",
    "tools_cmdTable": "node ./src/tools/createCmdTable.js",
    "tools_fly": "flyctl deploy",
    "web": "node ./src/index.js disable_announcer disable_bot"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crow08/discord-uberbot.git"
  },
  "dependencies": {
    "@discordjs/voice": "^0.11.0",
    "@discordjs/opus": "^0.8.0",
    "bufferutil": "^4.0.6",
    "discord.js": "^14.3.0",
    "erlpack": "^0.1.4",
    "ffmpeg-static": "^5.1.0",
    "libsodium": "^0.7.10",
    "mongodb": "^4.9.1",
    "node-opus": "^0.3.3",
    "node-spotify-api": "^1.1.1",
    "prism-media": "^1.3.4",
    "request": "^2.88.2",
    "utf-8-validate": "^5.0.9",
    "youtube-dl-exec": "^2.1.5",
    "ytdl-core": "^4.11.2",
    "ytpl": "^2.3.0",
    "zlib-sync": "^0.1.7",
    "zucc": "^0.1.2"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "discord-api-types": "^0.37.7",
    "eslint": "^8.23.0"
  }
}
