{
  "name": "uber-bot",
  "version": "0.0.1",
  "description": "Uber Discord Bot",
  "main": "uberbot",
  "author": "Crow08",
  "license": "MIT",
  "engines": {
    "node": "24.11.0",
    "npm": "11.6.2"
  },
  "scripts": {
    "announcer": "node ./src/index.js disable_bot disable_web",
    "bot": "node ./src/index.js disable_announcer disable_web",
    "build": "npm install",
    "debug": "node ./src/index.js debug",
    "docs": "node_modules/.bin/jsdoc -r -c jsdoc.json",
    "start": "node ./src/index.js",
    "tools_cmdTable": "node ./src/tools/createCmdTable.js",
    "tools_fly": "flyctl deploy",
    "web": "node ./src/index.js disable_announcer disable_bot"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crow08/discord-uberbot.git"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.9",
    "bufferutil": "^4.1.0",
    "discord.js": "^14.25.1",
    "erlpack": "^0.1.4",
    "ffmpeg-static": "^5.3.0",
    "libsodium": "^0.8.1",
    "mongodb": "^6.21.0",
    "node-opus": "^0.3.3",
    "node-spotify-api": "^1.1.1",
    "opusscript": "^0.0.8",
    "prism-media": "^1.3.5",
    "utf-8-validate": "^6.0.6",
    "youtube-dl-exec": "^3.0.29",
    "ytdl-core": "^4.11.5",
    "ytpl": "^2.3.0",
    "zlib-sync": "^0.1.10",
    "zucc": "^0.1.2"
  },
  "devDependencies": {
    "better-docs": "^2.7.3",
    "discord-api-types": "^0.38.37",
    "eslint": "^9.39.2"
  }
}
